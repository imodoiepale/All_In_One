{
   "author": "Simplify Jobs Inc.",
   "background": {
      "persistent": true,
      "scripts": [ "js/background.bundle.js" ]
   },
   "browser_action": {
      "chrome_style": false,
      "default_icon": {
         "128": "assets/icons/favicon-128.png",
         "16": "assets/icons/favicon-16.png",
         "32": "assets/icons/favicon-32.png",
         "48": "assets/icons/favicon-48.png"
      },
      "default_popup": "popup.html",
      "default_title": "Simplify Copilot"
   },
   "content_scripts": [ {
      "js": [ "js/dunderContentScript.bundle.js" ],
      "matches": [ "*://simplify.jobs/*", "*://*.simplify.jobs/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "js/contentScript.bundle.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_end"
   } ],
   "description": "The better way to job search. Quick apply and autofill job applications in one click. Track jobs, companies, resumes & more!",
   "differential_fingerprint": "1.610e50ca4e53baf1fe0168bc5a2912b72d19e86f19eb0fe049f80deae826a158",
   "externally_connectable": {
      "matches": [ "http://localhost/*", "https://simplify.jobs/*" ]
   },
   "homepage_url": "https://simplify.jobs/",
   "icons": {
      "128": "assets/icons/favicon-128.png",
      "16": "assets/icons/favicon-16.png",
      "32": "assets/icons/favicon-32.png",
      "48": "assets/icons/favicon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "78",
   "name": "Simplify Copilot - Autofill job applications",
   "permissions": [ "tabs", "activeTab", "cookies", "storage", "webNavigation", "webRequest", "http://*/*", "https://*/*" ],
   "short_name": "Simplify Jobs",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.5.17",
   "web_accessible_resources": [ "css/styles.css", "assets/*" ]
}
