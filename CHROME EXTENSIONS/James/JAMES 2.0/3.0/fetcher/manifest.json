
{
    "manifest_version": 3,
    "name": "Data Autofill Extension",
    "version": "1.0",
    "description": "Autofill data from Supabase",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}